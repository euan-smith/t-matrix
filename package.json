{
  "name": "t-matrix",
  "version": "0.0.4",
  "description": "A small type-array based matrix lib. A core matrix type plus just the other functions you need. ",
  "main": "index.js",
  "module": "index.mjs",
  "scripts": {
    "test": "mocha --require reify test/*.js",
    "cover": "nyc npm test",
    "build": "rollup -c",
    "prepublish": "npm run build",
    "docs": "jsdoc2md --template jsdoc2md/README.hbs --files src/*.js > README.md",
    "docs2": "jsdoc src/matrix.js src/core.js src/operations.js src/create.js src/manipulations.js src/tools.js src/conversions.js src/extras.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/zakalwe314/t-matrix.git"
  },
  "keywords": [
    "matrix",
    "typedArray",
    "float64",
    "linear",
    "algebra",
    "decomposition",
    "solver"
  ],
  "author": "Euan Smith <euan1971@yahoo.co.uk> (https://github.com/zakalwe314)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/zakalwe314/t-matrix/issues"
  },
  "homepage": "https://github.com/zakalwe314/t-matrix#readme",
  "devDependencies": {
    "array-flat-polyfill": "^1.0.1",
    "better-docs": "^1.1.6",
    "chai": "^4.2.0",
    "chai-almost": "^1.0.1",
    "jsdoc-to-markdown": "^5.0.0",
    "mocha": "^5.2.0",
    "nyc": "^14.1.1",
    "reify": "^0.17.2",
    "rollup": "^0.66.5",
    "rollup-plugin-commonjs": "^8.2.1",
    "rollup-plugin-json": "^3.1.0",
    "rollup-plugin-node-resolve": "^3.4.0",
    "rollup-plugin-terser": "^2.0.2"
  },
  "publishConfig": {
    "registry": "https://registry.npmjs.org/"
  }
}
