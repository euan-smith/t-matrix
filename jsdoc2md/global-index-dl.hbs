{{#globals kind=kind category=category ~}}
  {{#if @first~}}{{>heading-indent}}{{../title}}

  <dl>
  {{/if~}}
    <dt>{{../prefix}}{{>sig-link-html}}</dt>
  {{#if summary}}
    <dd>{{{md (inlineLinks summary)}}}</dd>
  {{else}}
    <dd>{{{md (inlineLinks description)}}}</dd>
  {{/if}}
  {{#if @last~}}</dl>

  {{/if~}}
{{/globals~}}